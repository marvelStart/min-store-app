.goods-scroll-container {
  width: 100%;
  height: calc(100vh - 128rpx - 100px);
}
.goods-scroll-container .stores-container {
  margin: 20rpx 20rpx 0;
  border-radius: 16rpx;
  background-color: #ffffff;
}
.goods-scroll-container .stores-container .store__title {
  display: flex;
  flex-direction: row;
  padding: 10rpx 20rpx;
}
.goods-scroll-container .stores-container .store__title text {
  margin-left: 20rpx;
  font-size: 24rpx;
}
.custom-submit-container {
  margin-bottom: 50px;
}
